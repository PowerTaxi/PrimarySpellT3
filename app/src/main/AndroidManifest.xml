<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.primary.spell" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name="com.primary.spell.WelcomeActivity"
            android:label="@string/app_name" >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.primary.spell.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.primary.spell.ExamActivity"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name="com.primary.spell.SettingsActivity"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name="com.primary.spell.ResultsActivity"
            android:label="@string/app_name" >
        </activity>

    </application>
    <uses-permission android:name="android.permission.INTERNET" />
</manifest>
